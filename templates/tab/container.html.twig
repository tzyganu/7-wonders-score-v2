<div id="{{ container.getHtmlId() }}">
    <ul>
        {% for index,tab in container.getTabs() %}
        <li><a href="#{{ index }}">{{ tab.getTitle() }}</a></li>
        {% endfor %}
    </ul>
    {% for index,tab in container.getTabs() %}
    <div id="{{ index }}">
        <p>{{ tab.getContent()|raw }}</p>
    </div>
    {% endfor %}
    <div class="clearfix"></div>
</div>
<script type="text/javascript">
    jQuery(document).ready(function($) {
        $('#{{ container.getHtmlId() }}').tabs();
    })
</script>
